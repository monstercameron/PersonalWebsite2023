  <body class="container mx-auto flex flex-col min-h-screen">
    <script type="">
      const printDiv = (divName) => {
        var printContents =
          document.getElementById(divName).shadowRoot.innerHTML;
        var originalContents = document.body.innerHTML;
        document.body.innerHTML = printContents;
        window.print();
        document.body.innerHTML = originalContents;
      };
      const print = () => {
        printDiv("resume");
      };
    </script>

